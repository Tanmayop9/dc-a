# Discord user token (not bot token)
USER_TOKEN=your_user_token_here

# Source server ID to clone from
SOURCE_SERVER_ID=source_server_id_here

# Target server ID to clone to
TARGET_SERVER_ID=target_server_id_here

# ── Optional / overridable at runtime via interactive prompts ──────────────────

# Comma-separated channel IDs to skip (channels that will NOT be cloned)
# SKIP_CHANNEL_IDS=123456789012345678,987654321098765432

# Maximum number of messages to clone per channel (default: 100)
# MSG_LIMIT=100

# Delay in milliseconds between API requests (default: 600)
# RATE_MS=600

# Number of channels to clone messages from simultaneously (default: 3)
# CONCURRENCY=3
